{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "aporat/store-receipt-validator": "^4.0",
        "doctrine/dbal": "^2.10",
        "firebase/php-jwt": "^5.0",
        "illuminate/redis": "^6.0",
        "ip2location/ip2location-laravel": "~1.0",
        "ixudra/curl": "6.17",
        "jenssegers/mongodb": "^3.5",
        "laravel/lumen-framework": "^6.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^2.7",
        "stevebauman/location": "^5.0",
        "symfony/cache": "^4.3",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "v10.2.2",
        "vlucas/phpdotenv": "^3.3",
        "guzzlehttp/psr7": "^1.8.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/migrations"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
